#include <stdio.h>

int main() {
	FILE *input_file, *output_file;
	char buffer[100];

input_file = fopen("crackme2.txt", "r");
	output_file = fopen("nano.txt", "w");

	while (fgets(buffer, 100, input_file) != NULL) {
	fputs(buffer, output_file);
	}

	fclose(input_file);
	fclose(output_file)
return 0;
}
